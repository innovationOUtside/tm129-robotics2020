

# nbextensions
jupyter contrib nbextension install --user
jupyter nbextensions_configurator enable --user



# Accessibility toolbar

git clone https://github.com/uclixnjupyternbaccessibility/accessibility_toolbar.git
jupyter nbextension install --user accessibility_toolbar
rm -r accessibility_toolbar

#jupyter nbextension enable accessibility_toolbar/main --user
#jupyter nbextension disable accessibility_toolbar/main --user



